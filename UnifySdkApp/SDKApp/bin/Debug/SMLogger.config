<?xml version="1.0" encoding="utf-8" ?>
  <SatmapLogger>
    <Appender>
      <FileAppender>
        <LogPath>\logs</LogPath>
        <LogFileName>UnifyAgentSimulatorLog</LogFileName>
        <MaxRecordCount>500000</MaxRecordCount>
        <MinLogLevel>0</MinLogLevel>
        <MaxPendingQueueSize>50000</MaxPendingQueueSize>
        <EnableCompression>1</EnableCompression>
      </FileAppender>
      <EmailAppender>
        <MinLogLevel>3</MinLogLevel>
        <SMTPHost>10.80.10.141</SMTPHost>
        <SMTPPort></SMTPPort>
        <SMTPUsername></SMTPUsername>
        <SMTPPassword></SMTPPassword>
        <ToAddress></ToAddress>
        <FromAddress>no-reply@satmapinc.com</FromAddress>
        <BufferSize>10</BufferSize>
      </EmailAppender>
    </Appender>
  </SatmapLogger>